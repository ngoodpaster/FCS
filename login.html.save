<html>

  <head>
    <title>firefightercomm</title   <link rel="stylesheet" href="m/bootstrap/3.3.7/css/bootstrap.min.css">
    <script src="js/lib/jquery.min.js" integrity=""></script>
    <link rel="stylesheet" type="text/css" href="/css/login.css"> <!--?version=1"> -->
    <!--<script src="login.js" type="text/javascript"></script>-->
    <script src="js/login.js"></script>
  </head>
  <body>
      <div id="regContainer" class="container">
            <div class="row">
            <div class="col-md-6 col-md-offset-3">
              <div class="panel panel-login">
                <div class="panel-heading">
                  <div class="row">
                    <div class="col-xs-6">
                      <a href="#" class="active" id="login-form-link">Login</a>
                    </div>
                    <div class="col-xs-6">
                      <a href="#" id="register-form-link">Register</a>
                    </div>
                  </div>
                  <hr>
                </div>
                <div class="panel-body">
                  <div class="row">
                    <div class="col-lg-12">
                      <form id="login-form" role="form" style="display: block;">
                        <div class="form-group">
                          <label class="control-label" for="username">Username</label>
                          <input type="text" name="username" id="username" tabindex="1" class="form-control" placeholder="Username">
                        </div>
                        <div class="form-group">
                          <label class="control-label" for="password">Password</label>
                          <input type="password" name="password" id="password" tabindex="2" class="form-control" placeholder="Password">
                        </div>
                        <div class="form-group text-center">
                        	<input type="radio" id="prevJobs" name="radOption" checked>
                        	<label for="prevJobs">Previous Jobs</label>
                        	<input type="radio" id="currJob" name="radOption">
                        	<label for="currJob">Current Job</label>
                          <!-- 
                          <input type="checkbox" tabindex="3" class="" name="remember" id="remember">
                          <label class="control-label" for="remember"> Remember Me</label> -->
                        </div>
                        <div class="form-group">
                          <div class="row">
                            <div class="col-sm-6 col-sm-offset-3">
                              <input type="button" name="login-submit" id="login-submit" tabindex="4" class="form-control btn btn-login" value="Log In">
                            </div>
                          </div>
                        </div>
                      </form>
                      <form id="register-form" role="form" style="display: none;">
                          <div class="form-group row"> 
                            <div class="col-xs-6">
                              <label class="control-label" for="username">Username: </label>
                              <input type="text" name="username" id="username-create" tabindex="1" class="form-control" placeholder="Username">
                            </div>
                            <div class="col-xs-6">
                              <label class="control-label" for="password">Password: </label>
                              <input type="password" name="password" id="password-create" tabindex="2" class="form-control" placeholder="Password">
                            </div>
                          </div>
                        <div class="form-group row">
                            <div class="col-xs-6"> 
                              <label class="control-label" for="first-name">First Name: </label>
                              <input type="text" name="first-name" id="first-name" tabindex="2" class="form-control" placeholder="First Name">
                            </div>
                            <div class="col-xs-6">
                              <label class="control-label" for="last-name">Last Name: </label>
                              <input type="text" name="last-name" id="last-name" tabindex="2" class="form-control" placeholder="Last Name">
                            </div>
                        </div>
                        <br>
                        <div class-"form-group row">
                          <div class="col-xs-3">
                            <label> Skills: </label>
                          </div>
                          <div class="col-xs-3">
                            <label class="checkbox-inline"><input id="cpr" type="checkbox">CPR</label>
                          </div>
                          <div class="col-xs-3">
                            <label class="checkbox-inline"><input id="first-aid" type="checkbox">First Aid</label>
                          </div>
                          <div class="col-xs-3">
                            <label class="checkbox-inline"><input id="driver" type="checkbox">Driver</label>
                          </div>
                        </div>
                        <br>
                        <br>
                        <br>
                        <br>
                        <div class="form-group">
                          <div class="row">
                            <div class="col-sm-6 col-sm-offset-3">
                              <input type="button" name="register-submit" id="register-submit" tabindex="4" class="form-control btn btn-register" value="Register Now">
                            </div>
                          </div>
                        </div>
                      </form>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
      </div>
  </body>
  </html>
